{
  "builds": [
    { "src": "pages/index.html", "use": "@vercel/static" }
        { "src": "api/*.js", "use": "@vercel/node" }

  ],
  "routes": [
    { "src": "/api/upload", "dest": "/api/upload.js" },
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/(.*)", "dest": "/pages/index.html" }
  ]
}
